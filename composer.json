{
  "name": "xamani/nova-grid-system",
  "description": "A Laravel Nova tool that allows placing fields in a grid using ->size() helpers.",
  "keywords": [
    "laravel",
    "nova",
    "grid",
    "layout"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Outl1ne\\NovaGrid\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Outl1ne\\NovaGrid\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
